{
  "name": "brewjournal-server",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "node ./dist/app.js",
    "tsc": "tsc",
    "postinstall": "npm run tsc",
    "dev": "node --require ts-node/register src/app.ts",
    "knex:migrate:latest": "npx knex --knexfile './src/knex/knexfile.ts' --knexpath './src/knex/knex.ts' migrate:latest",
    "knex:migrate:rollback": "npx knex --knexfile './src/knex/knexfile.ts' --knexpath './src/knex/knex.ts' migrate:rollback",
    "knex:migrate:up": "npx knex --knexfile './src/knex/knexfile.ts' --knexpath './src/knex/knex.ts' migrate:up",
    "knex:migrate:down": "npx knex --knexfile './src/knex/knexfile.ts' --knexpath './src/knex/knex.ts' migrate:down",
    "knex:seed": "npx knex --knexfile './src/knex/knexfile.ts' --knexpath './src/knex/knex.ts' seed:run"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.1",
    "express-openid-connect": "^2.7.3",
    "http-errors": "~1.6.3",
    "knex": "^3.1.0",
    "morgan": "~1.9.1",
    "pg": "^8.11.3",
    "pug": "^3.0.2"
  },
  "type": "commonjs",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@typescript-eslint/parser": "^6.19.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
